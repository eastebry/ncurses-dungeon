service nibbler
{
    protocol       = tcp
    disable        = no
    port           = 1338
    flags          = REUSE
    socket_type    = stream
    wait           = no
    user           = ctf
    server         = /home/ctf/cast
    env            = TERM=xterm-256color LINES=60 COLUMNS=200
    type           = UNLISTED
}
